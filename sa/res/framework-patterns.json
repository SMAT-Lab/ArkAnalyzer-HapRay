{
  "frameworks": {
    "RN": {
      "name": "React Native",
      "description": "React Native framework",
      "patterns": [
        "libreact_.*.so",
        "librnoh.so",
        "librnoh_.*.so"
      ]
    },
    "Flutter": {
      "name": "Flutter",
      "description": "Flutter framework",
      "patterns": [
        "libflutter.so"
      ]
    },
    "Hermes": {
      "name": "Hermes",
      "description": "Hermes JavaScript engine",
      "patterns": [
        "libhermes.so"
      ]
    },
    "KMP": {
      "name": "Kotlin Multiplatform",
      "description": "Kotlin Multiplatform framework",
      "patterns": [
        "libkn.so"
      ]
    },
    "CMP": {
      "name": "Compose Multiplatform",
      "description": "Compose Multiplatform framework",
      "patterns": [
        "libskikobridge.so"
      ]
    },
    "Lynx": {
      "name": "Lynx",
      "description": "Lynx framework",
      "patterns": [
        "liblynx.so",
        "liblynx_.*.so"
      ]
    },
    "Qt": {
      "name": "Qt",
      "description": "Qt framework",
      "patterns": [
        "libQt.*.so"
      ]
    },
    "Unity": {
      "name": "Unity",
      "description": "Unity game engine",
      "patterns": [
        "libunity.so",
        "libil2cpp.so"
      ]
    },
    "Taro": {
      "name": "Taro",
      "description": "Taro framework",
      "patterns": [
        "libJDSilkTaroNativeOH.so",
        "libSilkTaroKit.so",
        "libtaro_native_core.so",
        "libTaroHarmonyLibrary.so",
        "libsilktaronativelib.so"
      ]
    },
    "Weex": {
      "name": "Weex",
      "description": "Weex framework",
      "patterns": [
        "libweex_ability.so", 
        "libweex_framework.so"
      ]
    }
  },
  "fileTypes": {
    "ZIP": {
      "name": "ZIP Archive",
      "magic": [80, 75],
      "description": "ZIP compressed archive"
    },
    "PNG": {
      "name": "PNG Image",
      "magic": [137, 80, 78, 71, 13, 10, 26, 10],
      "description": "PNG image file"
    },
    "JPG": {
      "name": "JPEG Image",
      "magic": [255, 216, 255],
      "description": "JPEG image file"
    },
    "WEBP": {
      "name": "WebP Image",
      "magic": [82, 73, 70, 70],
      "description": "WebP image file"
    },
    "GIF": {
      "name": "GIF Image",
      "magic": [71, 73, 70, 56],
      "description": "GIF image file"
    },
    "BMP": {
      "name": "Bitmap Image",
      "magic": [66, 77],
      "description": "Bitmap image file"
    },
    "SO": {
      "name": "Shared Object",
      "magic": [127, 69, 76, 70],
      "description": "Shared object library"
    },
    "HERMES_BYTECODE": {
      "name": "Hermes Bytecode",
      "magic": [194, 31, 240, 159],
      "description": "Hermes JavaScript bytecode"
    },
    "WASM": {
      "name": "WebAssembly",
      "magic": [0, 97, 115, 109],
      "description": "WebAssembly binary file"
    },
    "PDF": {
      "name": "PDF Document",
      "magic": [37, 80, 68, 70],
      "description": "PDF document file"
    },
    "MP3": {
      "name": "MP3 Audio",
      "magic": [73, 68, 51],
      "description": "MP3 audio file"
    },
    "MP4": {
      "name": "MP4 Video",
      "magic": [0, 0, 0, 32, 102, 116, 121, 112],
      "magicOffset": 4,
      "description": "MP4 video file"
    },
    "WAV": {
      "name": "WAV Audio",
      "magic": [82, 73, 70, 70],
      "description": "WAV audio file"
    },
    "TTF": {
      "name": "TrueType Font",
      "magic": [0, 1, 0, 0],
      "description": "TrueType font file"
    },
    "OTF": {
      "name": "OpenType Font",
      "magic": [79, 84, 84, 79],
      "description": "OpenType font file"
    },
    "SQLITE": {
      "name": "SQLite Database",
      "magic": [83, 81, 76, 105, 116, 101, 32, 102, 111, 114, 109, 97, 116, 32, 51, 0],
      "description": "SQLite database file"
    },
    "RAR": {
      "name": "RAR Archive",
      "magic": [82, 97, 114, 33],
      "description": "RAR compressed archive"
    },
    "7Z": {
      "name": "7-Zip Archive",
      "magic": [55, 122, 188, 175, 39, 28],
      "description": "7-Zip compressed archive"
    }
  },
  "fileExtensions": {
    "zip": "ZIP",
    "png": "PNG",
    "jpg": "JPG",
    "jpeg": "JPG",
    "webp": "WEBP",
    "gif": "GIF",
    "bmp": "BMP",
    "so": "SO",
    "hbc": "HERMES_BYTECODE",
    "wasm": "WASM",
    "pdf": "PDF",
    "mp3": "MP3",
    "mp4": "MP4",
    "wav": "WAV",
    "ttf": "TTF",
    "otf": "OTF",
    "db": "SQLITE",
    "sqlite": "SQLITE",
    "sqlite3": "SQLITE",
    "rar": "RAR",
    "7z": "7Z",
    "js": "JS"
  }
}
